{
	"name": "nestor-bot",
	"displayName": "Nestor Bot",
	"version": "1.0.0-beta.2",
	"description": "A Discord Bot for tracking Fab Lab orders and manage your Servers.",
	"main": "build/index.js",
	"scripts": {
		"build": "tsc",
		"deploy": "node build/deploy-commands.js",
		"lint": "npx eslint .",
		"prestart": "npm run build && npm run deploy",
		"pretest": "ts-node src/deploy-commands.ts",
		"prettier": "prettier --write \"**/*.ts\"",
		"start": "node build/index.js",
		"test": "ts-node src/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GogoVega/nestor-bot"
	},
	"keywords": [
		"discord-bot",
		"order-tracking",
		"webhook"
	],
	"author": {
		"name": "Gauthier Dandele",
		"url": "https://github.com/GogoVega"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/GogoVega/nestor-bot/issues"
	},
	"homepage": "https://github.com/GogoVega/nestor-bot#readme",
	"dependencies": {
		"@discordjs/rest": "^1.1.0",
		"cpu-percentage": "^1.0.3",
		"discord-api-types": "^0.37.5",
		"discord.js": "^14.3.0",
		"winston": "^3.8.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.36.1",
		"@typescript-eslint/parser": "^5.36.1",
		"eslint": "^8.23.0",
		"prettier": "2.7.1",
		"typescript": "^4.8.2"
	}
}
